{
  "name": "webpack-playground",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "index.js",
  "author": "andorx <pngduc@gmail.com>",
  "license": "ISC",
  "config": {
    "webpack": "./cli/webpack.config.js",
    "cli": "cli/",
    "app": "app/"
  },
  "scripts": {
    "build": "bnr build",
    "start": "bnr start",
    "stats": "bnr stats",
    "validate:dev": "bnr validate:dev",
    "validate:prod": "bnr validate:prod",
    "lint": "bnr lint"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config $npm_package_config_webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config $npm_package_config_webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "stats": {
      "command": "webpack --config $npm_package_config_webpack --profile --json > stats.json"
    },
    "validate:dev": {
      "command": "webpack-validator $npm_package_config_webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "validate:prod": {
      "command": "webpack-validator $npm_package_config_webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint": {
      "command": "eslint $npm_package_config_app"
    }
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.13",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.10.1",
    "purifycss-webpack-plugin": "^2.0.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.16.3",
    "material-design-lite": "^1.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  }
}
